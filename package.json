{
  "name": "main-site-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
		"dev": "vite",
		"build": "tsc --build --clean && vite build",
		"start": "vite serve --host --port 5173",
		"cp": "node cli/cp.mjs",
		"setup": "npm i && pm2 start --name main-site npm -- start --update-env --time && pm2 save",
		"deploy": "git pull --no-rebase && pm2 restart main-site --update-env --time && pm2 save"
	},
  "dependencies": {
    "axios": "^1.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "sass": "^1.58.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@types/react-helmet": "^6.1.6",
    "@vitejs/plugin-react": "^2.2.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.0"
  }
}
